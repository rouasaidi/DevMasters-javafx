        <TextArea fx:id="descreptionF" layoutX="191.0" layoutY="216.0" prefHeight="162.0" prefWidth="386.0" />
        <Button layoutX="773.0" layoutY="80.0" mnemonicParsing="false" onAction="#addImage" prefHeight="26.0" prefWidth="183.0" text="Select" />
        <ImageView fx:id="imageview" fitHeight="308.0" fitWidth="372.0" layoutX="770.0" layoutY="196.0" pickOnBounds="true" preserveRatio="true" />
        <Button layoutX="882.0" layoutY="583.0" mnemonicParsing="false" onAction="#modifier" prefHeight="26.0" prefWidth="164.0" text="Save" />
        <Label fx:id="checkname" layoutX="202.0" layoutY="115.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="18.0" prefWidth="366.0" text="*Insert a valid name" textFill="RED" visible="false" />

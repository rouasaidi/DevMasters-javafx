<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.leila.Controller.User.ModiferDonnationController"
            prefHeight="700.0" prefWidth="1200.0">
    <children>
        <Label layoutX="102.0" layoutY="83.0" prefHeight="18.0" prefWidth="113.0" text="Name* :" />
        <Label layoutX="102.0" layoutY="207.0" prefHeight="18.0" prefWidth="78.0" text="Descreption* :" />

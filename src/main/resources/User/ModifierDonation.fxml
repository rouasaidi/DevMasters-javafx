        <Label layoutX="102.0" layoutY="149.0" prefHeight="18.0" prefWidth="65.0" text="Category* :" />
        <Label layoutX="102.0" layoutY="433.0" text="Quantity* :" />
        <Spinner fx:id="quantity" editable="true" layoutX="190.0" layoutY="438.0" prefWidth="386.0" />
        <Button layoutX="600.0" layoutY="583.0" mnemonicParsing="false" onAction="#goDonation" prefHeight="26.0" prefWidth="183.0" text="Cancel" />
        <Label fx:id="checkdescreption" layoutX="202.0" layoutY="388.0" prefHeight="18.0" prefWidth="366.0" text="*Insert a quaote" textFill="RED" visible="false" />
        <Label fx:id="checkimage" layoutX="648.0" layoutY="115.0" prefHeight="18.0" prefWidth="366.0" text="*Select Image" textFill="RED" visible="false" />
        <ComboBox fx:id="categoryCamcox" layoutX="190.0" layoutY="146.0" prefHeight="26.0" prefWidth="386.0" promptText="Select Category" />
    </children>
</AnchorPane>

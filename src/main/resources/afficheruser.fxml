<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="tableauafficher" prefHeight="400.0" prefWidth="600.0" stylesheets="login.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controls.afficheruser">

   <children>
      <TableView fx:id="tableview" layoutX="66.0" layoutY="73.0" prefHeight="537.0" prefWidth="996.0">
        <columns>
          <TableColumn fx:id="email" prefWidth="87.0" text="email" />
            <TableColumn fx:id="name" prefWidth="75.0" text="name" />
            <TableColumn fx:id="phone" prefWidth="86.0" text="phone" />
            <TableColumn fx:id="cin" prefWidth="62.0" text="cin" />
            <TableColumn fx:id="image" prefWidth="84.0" text="image" />
            <TableColumn fx:id="IS_verified" prefWidth="84.0" text="IS_verified" />
            <TableColumn fx:id="Reset_token" prefWidth="103.0" text="Reset_token" />
            <TableColumn fx:id="IS_banned" prefWidth="86.0" text="IS_banned" />
            <TableColumn fx:id="Roles" prefWidth="99.0" text="Roles" />
            <TableColumn fx:id="password" prefWidth="205.0" text="password" />
        </columns>
      </TableView>
      <Button fx:id="modifier" layoutX="312.0" layoutY="637.0" mnemonicParsing="false" onAction="#naviguer" prefHeight="25.0" prefWidth="252.0" text="modifier">
         <font>
            <Font name="Georgia Bold" size="14.0" />
         </font></Button>
      <Button fx:id="supprimer" layoutX="609.0" layoutY="637.0" mnemonicParsing="false" onAction="#supprimer" prefHeight="25.0" prefWidth="252.0" text="supprimer">
         <font>
            <Font name="Georgia Bold" size="14.0" />
         </font></Button>
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="ban" layoutX="124.0" layoutY="638.0" mnemonicParsing="false" onAction="#ban" text="ban" />
      <Button fx:id="excel" layoutX="912.0" layoutY="626.0" mnemonicParsing="false" onAction="#excel" text="excel" />
      <TextField fx:id="rehcercher" layoutX="517.0" layoutY="29.0" onAction="#rehcercher" promptText="rehchercher" />
   </children>
</AnchorPane>
